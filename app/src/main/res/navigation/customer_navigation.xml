<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/trip_navigation"
    app:startDestination="@id/fragment_time_line">

    <fragment
        android:id="@+id/fragment_time_line"
        tools:layout="@layout/fragment_time_line"
        android:name="ash.pickshunter.fragment.TimeLineFragment"
        android:label="TimeLineFragment" />

    <fragment
        android:id="@+id/fragment_product_details"
        tools:layout="@layout/fragment_product_details"
        android:name="ash.pickshunter.fragment.ProductDetailsFragment"
        android:label="ProductDetailsFragment" />

    <fragment
        android:id="@+id/fragment_check_out"
        tools:layout="@layout/fragment_check_out"
        android:name="ash.pickshunter.fragment.CheckOutFragment"
        android:label="CheckOutFragment" />

    <fragment
